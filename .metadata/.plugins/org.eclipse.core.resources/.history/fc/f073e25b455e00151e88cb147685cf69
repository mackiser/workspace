package snakeEyesExperiment;

public class snakeEyesExperiment {

	public static void main(String[] args) {
		System.out.println("This program performs an experiment to see how many times you have to roll a dice.");
		
		double dub1 = Math.floor((Math.random() * 6));
		double dub2 = Math.floor((Math.random() * 6));
		
		for (int i=0; i<=100; i++) {
			System.out.println("Roll " + i);
			System.out.println("Dice 1: " + Math.floor((Math.random() * 6)));
			System.out.println("Dice 2: " + Math.floor((Math.random() * 6)));
		}

	}

}
